{% extends '@OroUI/actions/index.html.twig' %}
{% import '@OroUI/macros.html.twig' as UI %}
{% set gridName = 'synolia-pokemon-pokemon-grid' %}
{% set pageTitle = 'synolia.pokemon.entity_plural_label'|trans %}

{% block navButtons %}
    {% import '@OroUI/macros.html.twig' as UI %}

    {% if is_granted('synolia_pokemon_pokemon_create') %}
        <div class="btn-group">
        {{ UI.addButton({
            'path': path('synolia_pokemon_pokemon_create'),
            'entity_label': 'synolia.pokemon.entity_label'|trans
        }) }}
        </div>
    {% endif %}
{% endblock %}
