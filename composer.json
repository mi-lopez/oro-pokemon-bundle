{
  "name": "mi-lopez/orocommerce-pokemon-bundle",
  "description": "Code used as an example for oro tech 7 using the oro integration package and the pokemon API https://pokeapi.co/",
  "license": "MIT",
  "authors": [
    {
      "name": "Miguel Lopez",
      "homepage": "https://github.com/mi-lopez"
    }
  ],
  "autoload": {
    "psr-4": {
      "Synolia\\Bundle\\PokemonBundle\\": "src"
    }
  },
  "repositories": {
    "composer": {
      "type": "composer",
      "url": "https://packagist.orocrm.com"
    }
  },
  "require": {
    "php": "~8.2.0",
    "oro/commerce": "5.1.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/flex": true,
      "symfony/runtime": true
    }
  }
}
